<?xml version="1.0"?>
<database xmlns="spryker:schema-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="zed"
          xsi:schemaLocation="spryker:schema-01 https://static.spryker.com/schema-01.xsd">

    <table name="pyz_test" idMethod="native">
        <column name="pyz_id_test" required="true" type="INTEGER" autoIncrement="true" primaryKey="true"/>
        <column name="prop_int" type="INTEGER"/>
        <column name="prop_string" size="100" type="VARCHAR"/>

        <unique name="pyz_test-prop_int">
            <unique-column name="prop_int"/>
        </unique>
        <id-method-parameter value="pyz_test_pk_seq"/>
    </table>

</database>
